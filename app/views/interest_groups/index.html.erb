<div class="container">

<h1>View all Interest Groups</h1>	

	<ul>
	<% @igroups.each do |i| %>
	<h1><%= i.name %></h1> <button> Subscribe</button>
	<% i.markers.each do |m| %>
	<li><%=m.title%></li>
	<% end %>
	<% end %>
	</ul>
	

	<%= link_to "Create New Interest Group", new_interest_group_path %>

</div>